{
    "name": "auto-json2dart",
    "displayName": "Auto-Json2Dart",
    "publisher": "zeuscore",
    "description": "Convert *.d.json to *.g.dart automatically ",
    "icon": "images/json2dart.png",
    "version": "1.1.0",
    "keywords": [
        "json",
        "generate",
        "convert",
        "dart model",
        "automatically"
    ],
    "author": "zeuscore",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/zeusCore/json2dart-vscode-plugin.git"
    },
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "auto-json2dart.convertAllDJsonToDart",
                "title": "auto-json2dart: Convert all *.d.json to *.g.dart"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "compile": "webpack --config ./build/node-extension.webpack.config.js",
        "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
        "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
        "test-compile": "tsc -p ./",
        "test-watch": "tsc -watch -p ./",
        "pretest": "yarn run test-compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/mockjs": "^1.0.3",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "change-case": "^4.1.2",
        "de-indent": "^1.0.2",
        "ejs-loader": "^0.5.0",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "he": "^1.2.0",
        "mocha": "^8.1.3",
        "ts-loader": "^8.0.11",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1",
        "webpack": "^5.10.0",
        "webpack-cli": "^4.2.0"
    },
    "dependencies": {
        "@j2a/core": "^1.0.6",
        "mockjs": "^1.1.0"
    }
}
